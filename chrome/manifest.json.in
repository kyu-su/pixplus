{
  "name": "pixplusPlus",
  "version": "@VERSION@",
  "manifest_version": 2,
  "description": "hogehoge",
  "update_url": "https://github.com/kyu-su/pixplusPlus/blob/master/autoupdate/1/chrome.xml",
  "icons": {
    @ICONS@
  },
  "background": {
    "page": "index.html"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["http://www.pixiv.net/*", "https://www.pixiv.net/*"],
      "js": ["pixplusPlus.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
